<h1> Create New User </h1>
<%= form_for @user do |u| %>
    <br> First Name: <%= u.text_field :first_name %></br>
    <br> Last Name: <%= u.text_field :last_name %></br>
    <br> Username: <%= u.text_field :username %></br>
    <br> Email: <%= u.email_field :email %></br>
    <br> Password: <%= u.password_field :password %></br>
    <br> Password Confirmation: <%= u.password_field :password_confirmation %></br>
    <br><%= u.submit %> </br>
<% end %>